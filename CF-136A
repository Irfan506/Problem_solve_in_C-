#include <bits/stdc++.h>
using namespace std;
int main() {
    int n,e;
    cin>>n;
    int arr[n+2];
    int x[n+15];
    for (int i=1;i<=n;i++) {
        cin>>x[i];
    }
    for (int i=1;i<=n;i++) {
        arr[x[i]]=i;
    }
    for (int i=1;i<=n;i++) {
        cout<<arr[i]<<" ";
    }
    return 0;
}
