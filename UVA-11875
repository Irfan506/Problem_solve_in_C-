#include<bits/stdc++.h>
using namespace  std;
int main()
{
    int t;
    cin>>t;
    int n,c,flag=0;
    while(t--)
    {
        cin>>n;
        for (int i=0;i<n;i++)
        {
            cin>>c;
            if (i==n/2)
                cout <<"Case "<<++flag<< ": "<<c<<endl;
        }
    }
    return 0;
}
